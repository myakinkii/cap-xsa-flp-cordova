<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:l="sap.ui.layout"
		xmlns:f="sap.ui.layout.form"
		xmlns:u="sap.ui.unified"
		xmlns:core="sap.ui.core">
	<Dialog title="{i18n>workoutPlanExDlgTitle}">
		<beginButton>
			<Button text="{i18n>genericSave}" type="Emphasized" press="submitChanges"/>
		</beginButton>
		<endButton>
			<Button text="{i18n>genericCancel}" press="closeDlg"/>
		</endButton>
		<VBox class="sapUiSmallMargin">
			<core:Fragment fragmentName="ru.fitrepublic.shared.fragments.ExcercisePlanForm" type="XML" />
		</VBox>
	</Dialog>
</core:FragmentDefinition>