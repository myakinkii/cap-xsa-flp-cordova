{
	"_version": "1.5.0",
	"sap.app": {
		"id": "ru.fitrepublic.ui_fe_gyms",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/v2/admin",
				"type": "OData",
				"settings": {
					"annotations": [
						"localAnnotations"
					],
					"localUri": "localService/metadata.xml"
				}
			},
			"localAnnotations": {
				"uri": "annotations/annotations.xml",
				"type": "ODataAnnotation",
				"settings": {
					"localUri": "annotations/annotations.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ru.fitrepublic.ui_fe_gyms.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"defaultUpdateMethod": "PATCH",
					"useBatch": true,
					"refreshAfterChange": false
				}
			},
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			}
		},
        "extends": {
            "extensions": {
                "sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"BeforeFacet|Gyms|coaches::com.sap.vocabularies.UI.v1.LineItem::NAV": {
							"className": "sap.ui.core.Fragment",
							"fragmentName": "ru.fitrepublic.ui_fe_gyms.ext.fragments.DescriptionBreakout",
							"type": "XML",
							"sap.ui.generic.app": { "title": "{{DescriptionFacet}}" }
						}
					}
                }
            }
        },
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	},
	"sap.ui.generic.app": {
		"pages": [{
			"entitySet": "Gyms",
			"component": {
				"settings": {
					"smartTable": true,
					"multiSelect": false
				},
				"name": "sap.suite.ui.generic.template.ListReport",
				"list": true
			},
			"pages": [{
				"entitySet": "Gyms",
				"component": {
					"name": "sap.suite.ui.generic.template.ObjectPage"
				},
				"_pages": [{
					"navigationProperty": "coaches",
					"entitySet": "CoachesToGyms",
					"component": {
						"name": "sap.suite.ui.generic.template.ObjectPage"
					}
				}]
			}]
		}]
	}
}